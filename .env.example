# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ratings_db?schema=public

# Kafka
KAFKA_URL=localhost:9092
KAFKA_GROUP_ID=member-profile-processor

# Kafka Topics (used by KafkaHandlerService)
MARATHON_RATING_TOPIC=marathon.rating.calculate
AGORITHM_RATING_TOPIC=algorithm.rating.calculate

# Kafka Subscription Topics (used by app.ts consumer)
KAFKA_AUTOPILOT_NOTIFICATIONS_TOPIC=notifications.autopilot
KAFKA_RATING_SERVICE_TOPIC=rating.calculation.service

# Logging
LOG_LEVEL=info

# Auth0 (for V5 API calls - challenge lookup and submission fetching)
AUTH0_URL=
AUTH0_AUDIENCE=
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
TOKEN_CACHE_TIME=86400000

# V5 API
V5_API_URL=https://api.topcoder-dev.com/v5

# Member API (optional)
MEMBER_API_URL=

# Health check
HEALTHCHECK_PORT=3000
